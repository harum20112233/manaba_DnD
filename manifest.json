{
  "manifest_version": 3,
  "name": "manabaドラッグアンドドロップ",
  "version": "1.0",
  "description": "manabaの課題提出ページのドラッグアンドドロップがやりやすくなる拡張機能",
  "host_permissions": ["https://*/ct/course_*_report_*"],
  "content_scripts": [
    {
      "matches": ["https://*/ct/course_*_report_*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
